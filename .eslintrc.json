{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@typescript-eslint/parser"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
      "semi": [2, "never"],
      "quotes": [2, "single"]
    }
}
